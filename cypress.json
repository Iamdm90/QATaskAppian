{
    "defaultCommandTimeout": 8000,
    "pageLoadTimeout": 60000,
    "viewportWidth": 1920,
    "viewportHeight": 1080,
    "retries": {
        "runMode": 0,
        "openMode": 0
      },
    "testFiles": "**/*.js",
    "reporter": "cypress-multi-reporters",
    "reporterOptions": {
        "reporterEnabled": "mochawesome",
        "mochawesomeReporterOptions": {
            "reportDir": "cypress/reports/mocha",
            "quite": true,
            "overwrite": false,
            "html": false,
            "json": true
        }
    },
    "env":{
        "url": "https://www.google.com"
    }
}

